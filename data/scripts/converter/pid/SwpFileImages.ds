namespace TresGoesDE.Converting


pin Dragengine.Gui


/**
 * Swp file  images.
 */
class SwpFileImages
	private var EditableImage pColor
	private var EditableImage pOpacity
	private var EditableImage pBumpMap
	
	
	public func new( EditableImage color, EditableImage opacity, EditableImage bumpMap )
		pColor = color
		pOpacity = opacity
		pBumpMap = bumpMap
	end
	
	
	public func EditableImage getColor()
		return pColor
	end
	
	public func EditableImage getOpacity()
		return pOpacity
	end
	
	public func EditableImage getBumpMap()
		return pBumpMap
	end
end
