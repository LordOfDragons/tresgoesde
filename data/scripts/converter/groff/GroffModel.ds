namespace TresGoesDE.Converting


/**
 * Groff model.
 */
class GroffModel
	private var int pId
	private var GroffName pName
	private var GroffGeometry pGeometry
	private var GroffMapping pMapping
	private var Float pBaseScale
	
	
	public func new()
		pId = 0
	end
	
	
	public func int getId()
		return pId
	end
	
	public func void setId( int id )
		pId = id
	end
	
	public func GroffName getName()
		return pName
	end
	
	public func void setName( GroffName name )
		pName = name
	end
	
	public func GroffGeometry getGeometry()
		return pGeometry
	end
	
	public func void setGeometry( GroffGeometry geometry )
		pGeometry = geometry
	end
	
	public func GroffMapping getMapping()
		return pMapping
	end
	
	public func void setMapping( GroffMapping mapping )
		pMapping = mapping
	end
	
	public func Float getBaseScale()
		return pBaseScale
	end
	
	public func void setBaseScale( Float baseScale )
		pBaseScale = baseScale
	end
end
